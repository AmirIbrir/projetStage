{% extends 'base.html.twig' %}
{% block body %}

<!DOCTYPE html>
<html lang="en">

  <head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="TemplateMo">
    <link href="https://fonts.googleapis.com/css?family=Poppins:100,200,300,400,500,600,700,800,900&display=swap" rel="stylesheet">

    <title>Labella Business - Contact Page</title>

    <!-- Bootstrap core CSS -->
    <link href="vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Additional CSS Files -->
    <link rel="stylesheet" href="assets/css/fontawesome.css">
    <link rel="stylesheet" href="assets/css/templatemo-finance-business.css">
    <link rel="stylesheet" href="assets/css/owl.css">
<!--

Finance Business TemplateMo

https://templatemo.com/tm-545-finance-business

-->
  </head>

  <body>

    <!-- ***** Preloader Start ***** -->
    <div id="preloader">
        <div class="jumper">
            <div></div>
            <div></div>
            <div></div>
        </div>
    </div>  
    <!-- ***** Preloader End ***** -->



    <!-- Page Content -->
    <div class="page-heading header-text">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <h1>Contactez-nous</h1>
            <span>N'HÉSITEZ PAS À NOUS ENVOYER UN MESSAGE DÈS MAINTENANT !</span>
          </div>
        </div>
      </div>
    </div>

    <div class="contact-information">
      <div class="container">
        <div class="row">
          <div class="col-md-4">
            <div class="contact-item">
              <i class="fa fa-phone"></i>
              <h4>téléphone</h4><br><br><br><br>
              <p></p>
              <a href="#">01.02.03.04.05</a>
            </div>
          </div>
          <div class="col-md-4">
            <div class="contact-item">
              <i class="fa fa-envelope"></i>
              <h4>Email</h4><br><br><br><br>
              <p></p>
              <a href="#">info@labelab.com</a>
            </div>
          </div>
          <div class="col-md-4">
            <div class="contact-item h-100">
              <i class="fa fa-map-marker"></i>
              <h4>Position</h4>
              <p>Sharjah Media City (Shams), AlMessaned, AlBataeh, Sharjah, </br>United Arab Emirates.</p>
              <a href="https://www.google.fr/maps/place/Charjah+-+Émirats+arabes+unis/@25.3199211,55.4053541,11z/data=!3m1!4b1!4m5!3m4!1s0x3e5f5f5fede7964b:0x2a830aa19c1f6d89!8m2!3d25.3461555!4d55.4210932?hl=fr&authuser=0">View on Google Maps</a></br>
            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="callback-form contact-us">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <div class="section-heading">
              <h2>Demander à  <em>être rappelé</em></h2>
              <span>Laissez-nous vos coordonnées, nous vous rappelons sous 48h</span>
            </div>
          </div>
          <div class="col-md-12">
            <div class="contact-form">
            <!-- Remplace la balise form ouvrante -->
            {{form_start(formMessage, { attr : { "data-controller" : "postform2-ajax"} })}}
              <!--<form id="contact" action="" method="get">-->
                <div class="row">
                  <div class="col-lg-4 col-md-12 col-sm-12">
                    <fieldset>
                      <!--<input name="name" type="text" class="form-control" id="name" placeholder="Full Name" required="">-->
                      {{ form_widget( formMessage.fullName, { attr : { "class" : "form-control", "placeholder" : "Full name" } } ) }}
                    </fieldset>
                  </div>
                  <div class="col-lg-4 col-md-12 col-sm-12">
                    <fieldset>
                      <!--<input name="email" type="text" class="form-control" id="email" pattern="[^ @]*@[^ @]*" placeholder="E-Mail Address" required="">-->
                      {{ form_widget( formMessage.email, { attr : { "class" : "form-control", "placeholder" : "E-Mail" } } ) }}
                    </fieldset>
                  </div>
                  <div class="col-lg-4 col-md-12 col-sm-12">
                    <fieldset>
                      <!--<input name="subject" type="text" class="form-control" id="subject" placeholder="Subject" required="">-->
                      {{ form_widget( formMessage.subject, { attr : { "class" : "form-control", "placeholder" : "Subject" } } ) }}
                    </fieldset>
                  </div>
                  <div class="col-lg-12">
                    <fieldset>
                      <!--<textarea name="message" rows="6" class="form-control" id="message" placeholder="Your Message" required=""></textarea>-->
                      {{ form_widget( formMessage.messageContent, { attr : { "class" : "form-control", "placeholder" : "Your message" } } ) }}
                    </fieldset>
                  </div>
                  <div class="col-lg-12">
                    <fieldset>
                      <button type="submit" id="form-submit" class="filled-button">Envoyer</button>
                    </fieldset>
                  </div>
                </div>
                 {{form_end(formMessage)}}

              <!--</form>-->
             
            </div>
          </div>
        </div>
      </div>
    </div> 

   
     

    <div id="map">
<!-- How to change your own map point
	1. Go to Google Maps
	2. Click on your location point
	3. Click "Share" and choose "Embed map" tab
	4. Copy only URL and paste it within the src="" field below
--> 
<iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d230818.05220314613!2d55.405354104373934!3d25.31992105880843!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3e5f5f5fede7964b%3A0x2a830aa19c1f6d89!2sCharjah%20-%20%C3%89mirats%20arabes%20unis!5e0!3m2!1sfr!2sfr!4v1661581358432!5m2!1sfr!2sfr" width="100%" height="500" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>    </div>

    <div class="partners contact-partners">
      <div class="container">
        <div class="row">
          <!--<div class="col-md-12">
            <div class="owl-partners owl-carousel">
              <div class="partner-item">
                <img src="assets/images/client-01.png" alt="">
              </div>
            </div>-->
          </div>
        </div>
      </div>
    </div>


    <!-- Footer Starts Here -->
    {% include "home/sections/footer.html.twig" %}
            
            <!--- Design: <a rel="nofollow noopener" href="https://templatemo.com" target="_blank">TemplateMo</a><br>
            Distributed By: <a rel="nofollow noopener" href="https://themewagon.com" target="_blank">ThemeWagon</a>
          </p>-->
          </div>
        </div>
      </div>
    </div>

    <!-- Bootstrap core JavaScript -->
    <script src="vendor/jquery/jquery.min.js"></script>
    <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Additional Scripts -->
    <script src="assets/js/custom.js"></script>
    <script src="assets/js/owl.js"></script>
    <script src="assets/js/slick.js"></script>
    <script src="assets/js/accordions.js"></script>

    <script language = "text/Javascript"> 
      cleared[0] = cleared[1] = cleared[2] = 0; //set a cleared flag for each field
      function clearField(t){                   //declaring the array outside of the
      if(! cleared[t.id]){                      // function makes it static and global
          cleared[t.id] = 1;  // you could use true and false, but that's more typing
          t.value='';         // with more chance of typos
          t.style.color='#fff';
          }
      }
    </script>

  </body>
</html>

{% endblock %}
